entity Reward {
    createdAt ZonedDateTime required,
    fromDate ZonedDateTime required,
    toDate ZonedDateTime required,
    code String required unique,
    status RewardStatus required
}

enum RewardStatus {
    ACTIVE, EXPIRED, USED
}

relationship ManyToOne{
    Reward{user required} to User{rewards} with builtInEntity,
    Reward{rewardConfig required} to RewardConfig with builtInEntity,
}

dto * with mapstruct paginate Reward with pagination
